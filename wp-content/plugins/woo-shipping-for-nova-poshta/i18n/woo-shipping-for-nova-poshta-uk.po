msgctxt "Регион"
msgid "Region"
msgstr "Регіон"

#: ../classes/Checkout.php:102 ../classes/WC_NovaPoshta_Shipping_Method.php:88
msgctxt "Город"
msgid "City"
msgstr "Місто"

#: ../classes/Checkout.php:111 ../classes/WC_NovaPoshta_Shipping_Method.php:99
msgctxt "Склад Новой Почты (№)"
msgid "Nova Poshta Warehouse (#)"
msgstr "Склад Нової Пошти (№)"

